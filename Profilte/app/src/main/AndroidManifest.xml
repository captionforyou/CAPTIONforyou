<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.profilte">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".showCaptionBoard"></activity>
        <activity android:name=".noticeActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 팝업 Activity -->
        <activity android:name=".AskingConfirmPopUp" android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".noticePopUp" android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".RatePopUp" android:theme="@android:style/Theme.Dialog" />
    </application>

</manifest>